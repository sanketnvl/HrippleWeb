$(document).ready(function() {
    
    $('#sendEmailWithResume').on('click', function() {
     
    	var valid = $("#defaultForm1").valid();
    	if(valid ==true){
        var formData = new FormData();
        formData.append('name', $("#name").val());
        formData.append('loc', $("#loc").val());
        formData.append('add', $("#add").val());
        formData.append('dob', $("#example1").val());
        formData.append('co', $("#co").val());
        formData.append('cd', $("#cd").val());
        formData.append('le', $("#le").val());
        formData.append('cn', $("#cn").val());
        formData.append('cs', $("#cs").val());
        formData.append('message', $("#message").val());
        formData.append('email', $("#email").val());
        
        // Attach file
        formData.append('file', $('input[type=file]')[0].files[0]); 
        
        $.ajax({
            url: 'sendEmailWithResume.html',
            type: "POST",
            data: formData,
            enctype: 'multipart/form-data',
            processData: false,
            contentType: false,
            beforeSend : function() {
    			$('#sendEmailWithResume').buttonLoader('start');
    		},
    		complete : function() {
    			$("#sendEmailWithResume").buttonLoader('stop');
    		},
            success:function(response){
           		//alert("success");
           		if(response == "success"){
           			
           			
           			$.confirm({
           	    		title: '<strong>Success</strong>',
           	     	    content: 'You have successfully upload your resume.we will get back to you shortly!',
           	     	    type: 'orange',
           	    	    typeAnimated: true,
           	    	 	
           	    	    buttons: {
           	    	     somethingElse: {
           	              text: 'Ok',
           	              keys: ['enter', 'shift'],
           	              action: function(){
           	            	location.reload();
           	              }
           	        	  },
           	    	        close: function () {
           	    	        	location.reload();
           	    	        }
           	    	    }
           			});
           		}
            },
		    error: function(err){
		    	alert(err);
		    }
        }); 
		 }
        
   	 });
    
 
});
    
</script>
	 
